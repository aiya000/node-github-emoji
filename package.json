  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/node-github-emoji.git"
  },
  "keywords": [
    "GitHub",
    "emoji"
  ],
  "author": "rhysd <https://github.com/rhysd>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/node-github-emoji/issues"
  },
  "homepage": "https://github.com/rhysd/node-github-emoji#readme",
  "devDependencies": {
    "@types/node": "^10.12.10",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.1.6"
  }
}
