{
  "name": "github-emoji",
  "version": "0.0.0",
  "description": "GitHub emoji collection",
  "main": "index.js",
  "scripts": {
    "dts": "tsc -p src -d",
    "build": "tsc -p src",
    "watch": "tsc -p src --watch",
    "update-emoji": "bash ./scripts/update-emooji.sh",
    "update": "npm-run-all update-emoji build dts",
